<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Templates">
        <folder name="Project">
            <folder name="OPS">
                <file name="NewOPSIDLProjectProject.zip" url="NewOPSIDLProjectProject.zip">
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="ops.netbeansmodules.sampleprojects.Bundle"/>
                    <attr name="instantiatingIterator" methodvalue="ops.netbeansmodules.sampleprojects.NewOPSIDLProjectWizardIterator.createIterator"/>
                    <attr name="instantiatingWizardURL" urlvalue="nbresloc:/ops/netbeansmodules/sampleprojects/NewOPSIDLProjectDescription.html"/>
                    <attr name="template" boolvalue="true"/>
                </file>
            </folder>
        </folder>
        <folder name="XML">
            <file name="oPSConfigFile">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="ops.netbeansmodules.sampleprojects.Bundle"/>
                <attr name="instantiatingIterator" newvalue="ops.netbeansmodules.sampleprojects.OPSConfigFileWizardIterator"/>
                <attr name="template" boolvalue="true"/>
                <attr name="templateWizardURL" urlvalue="nbresloc:/ops/netbeansmodules/sampleprojects/oPSConfigFile.html"/>
            </file>
        </folder>
    </folder>
</filesystem>
